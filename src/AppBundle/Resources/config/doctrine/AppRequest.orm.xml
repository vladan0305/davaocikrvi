<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AppRequest" table="app_request">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idUser" type="integer" column="id_user" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="dateTime" type="datetime" column="date_time" nullable="false"/>
    <field name="bloodType" type="string" column="blood_type" length="5" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="targetName" type="string" column="target_name" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="timeActive" type="datetime" column="time_active" nullable="false"/>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <field name="changedStatusTime" type="datetime" column="changed_status_time" nullable="false"/>
    <field name="changedById" type="integer" column="changed_by_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
